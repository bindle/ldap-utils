//
//  Microsoft: Open Specifications
//  3.1.1.3.4.4 LDAP Matching Rules (extensibleMatch)
//  https://docs.microsoft.com/en-us/openspecs/windows_protocols/ms-adts/4e638665-f466-4597-93c4-12f2ebfabab5
//
//  The following sections describe the matching rules supported by DCs when
//  performing LDAP search requests. Unlike, for example, extended controls
//  and extended operations, there is no attribute exposed by the DC that
//  specifies which matching rules it supports. The identifiers for these
//  matching rules are used in an extensibleMatch clause in the filter
//  portion of a SearchRequest, as described in [RFC2251] section 4.5.1.
//  Matching rules are identified by an OID that corresponds to a
//  human-readable name, as shown in the following table.
//
//   Capability name                     | OID
//  -------------------------------------+-------------------------
//   LDAP_MATCHING_RULE_BIT_AND          | 1.2.840.113556.1.4.803
//   LDAP_MATCHING_RULE_BIT_OR           | 1.2.840.113556.1.4.804
//   LDAP_MATCHING_RULE_TRANSITIVE_EVAL  | 1.2.840.113556.1.4.1941
//   LDAP_MATCHING_RULE_DN_WITH_DATA     | 1.2.840.113556.1.4.2253
//
{
   .oid           =  "1.2.840.113556.1.4.803",
   .name          =  "LDAP_MATCHING_RULE_BIT_AND",
   .desc          =  "LDAP_MATCHING_RULE_BIT_AND",
   .flags         =  0,
   .type          =  LDAPSCHEMA_MATCHINGRULE,
   .def           =  NULL,
   .spec          =  "Microsoft: Open Specifications",
   .spec_type     =  LDAPSCHEMA_SPEC_URL,
   .spec_name     =  "Open Specifications",
   .spec_section  =  "3.1.1.3.4.4 LDAP Matching Rules (extensibleMatch)",
   .spec_source   =  "https://docs.microsoft.com/en-us/openspecs/windows_protocols/ms-adts/4e638665-f466-4597-93c4-12f2ebfabab5",
   .spec_vendor   =  "Microsoft",
   .spec_text     =  "This rule is equivalent to a bitwise \"AND\" operation."
                     " When this matching rule is used as a clause in a query"
                     " filter, the clause is satisfied only if all the bits"
                     " set to '1' in the value included in the clause"
                     " correspond to bits set to '1' in the value stored in"
                     " the directory.\n",
   .notes         =  NULL,
};
{
   .oid           =  "1.2.840.113556.1.4.804",
   .name          =  "LDAP_MATCHING_RULE_BIT_OR",
   .desc          =  "LDAP_MATCHING_RULE_BIT_OR",
   .flags         =  0,
   .type          =  LDAPSCHEMA_MATCHINGRULE,
   .def           =  NULL,
   .spec          =  "Microsoft: Open Specifications",
   .spec_type     =  LDAPSCHEMA_SPEC_URL,
   .spec_name     =  "Open Specifications",
   .spec_section  =  "3.1.1.3.4.4 LDAP Matching Rules (extensibleMatch)",
   .spec_source   =  "https://docs.microsoft.com/en-us/openspecs/windows_protocols/ms-adts/4e638665-f466-4597-93c4-12f2ebfabab5",
   .spec_vendor   =  "Microsoft",
   .spec_text     =  "This rule is equivalent to a bitwise \"OR\" operation."
                     " When this matching rule is used as a clause in a query"
                     " filter, the clause is satisfied only if at least one of"
                     " the bits set to '1' in the value included in the clause"
                     " corresponds to a bit set to '1' in the value stored in"
                     " the directory.\n",
   .notes         =  NULL,
};
{
   .oid           =  "1.2.840.113556.1.4.1941",
   .name          =  "LDAP_MATCHING_RULE_TRANSITIVE_EVAL",
   .desc          =  "LDAP_MATCHING_RULE_TRANSITIVE_EVAL",
   .flags         =  0,
   .type          =  LDAPSCHEMA_MATCHINGRULE,
   .def           =  NULL,
   .spec          =  "Microsoft: Open Specifications",
   .spec_type     =  LDAPSCHEMA_SPEC_URL,
   .spec_name     =  "Open Specifications",
   .spec_section  =  "3.1.1.3.4.4 LDAP Matching Rules (extensibleMatch)",
   .spec_source   =  "https://docs.microsoft.com/en-us/openspecs/windows_protocols/ms-adts/4e638665-f466-4597-93c4-12f2ebfabab5",
   .spec_vendor   =  "Microsoft",
   .spec_text     =  "This rule provides recursive search of a link attribute."
                     " A filter F of the form \"(A: 1.2.840.113556.1.4.1941:=V)\","
                     " where A is a link attribute and V is a value, evaluates to"
                     " True for an object whose DN is D if the following method"
                     " EvalTransitiveFilter(A, V, D) returns true, and False"
                     " if the method returns false. If A is not a link"
                     " attribute, the filter F evaluates to Undefined.\n"
                     "\n"
                     "EvalTransitiveFilter(A: attribute, V: value, D: DN)\n"
                     "\n"
                     "   *  If A is of Object(DN-String), Object(DN-Binary),"
                     " Object(OR-Name), or Object(Access-Point) syntax, let V'"
                     " equal the object_DN portion of V. Otherwise, let V'"
                     " equal V\n"
                     "\n"
                     "   * Return the value of EvalTransitiveFilterHelper(A,"
                     " V', D, {})\n"
                     "\n"
                     "EvalTransitiveFilterHelper(A: attribute, V': value,"
                     " ToVisit: DN, Visited: SET OF DN)\n"
                     "\n"
                     "   * If A is of Object(DN-String), Object(DN-Binary),"
                     " Object(OR-Name), or Object(Access-Point) syntax, let C"
                     " be the set of the object_DN components of the values"
                     " of ToVisit.A. Otherwise, let C be the set of the"
                     " values of ToVisit.A. Note that C is a set of DNs.\n"
                     "\n"
                     "   * If V' is in C, return true.\n"
                     "\n"
                     "   * Let Visited' equal the Visited set plus {ToVisit}.\n"
                     "\n"
                     "   * For each DN NextToVisit in C\n"
                     "\n"
                     "     - If NextToVisit is in Visited, do nothing.\n"
                     "\n"
                     "     - Let Result = EvalTransitiveFilterHelper(A, V', NextToVisit, Visited')\n"
                     "\n"
                     "     - If Result is true, return true.\n"
                     "\n"
                     "   * Return false.\n",
   .notes         =  NULL,
};
{
   .oid           =  "1.2.840.113556.1.4.2253",
   .name          =  "LDAP_MATCHING_RULE_DN_WITH_DATA",
   .desc          =  "LDAP_MATCHING_RULE_DN_WITH_DATA",
   .flags         =  0,
   .type          =  LDAPSCHEMA_MATCHINGRULE,
   .def           =  NULL,
   .spec          =  "Microsoft: Open Specifications",
   .spec_type     =  LDAPSCHEMA_SPEC_URL,
   .spec_name     =  "Open Specifications",
   .spec_section  =  "3.1.1.3.4.4 LDAP Matching Rules (extensibleMatch)",
   .spec_source   =  "https://docs.microsoft.com/en-us/openspecs/windows_protocols/ms-adts/4e638665-f466-4597-93c4-12f2ebfabab5",
   .spec_vendor   =  "Microsoft",
   .spec_text     =  "This rule provides a way to match on portions of values"
                     " of syntax Object(DN-String) and Object(DN-Binary).\n"
                     "\n"
                     "Let F be a filter of the form \"(A: 1.2.840.113556.1.4.2253:=V)\","
                     " where A is a link attribute and V is a value of syntax"
                     " Object(DN-String) (section 3.1.1.2.2.2.1) or"
                     " Object(DN-Binary) (section 3.1.1.2.2.2.3). This filter"
                     " evaluates to True for an object whose DN is D if the"
                     " method defined below, EvalDNWithDataFilter(A,V,D),"
                     " returns true, and False if the method returns false."
                     " If A is not of syntax Object(DN-String) or"
                     " Object(DN-Binary), the filter F evaluates to Undefined.\n"
                     "\n"
                     "EvalDNWithDataFilter(A: attribute, V: value, D: DN)\n"
                     "\n"
                     "   * For either syntax, let O be the DN portion of the"
                     " value V and B be the string or binary portion of the"
                     " value V. If the attribute is of syntax"
                     " Object(DN-String), B is the value of the string"
                     " considered strictly as the sequence of bytes of the"
                     " string. Note that O can be the rootDSE. Note also that"
                     " B can have 0 length.\n"
                     "\n"
                     "   * For every V' where V' is a value of attribute A on"
                     " object D:\n"
                     "\n"
                     "     - Let O' be the DN portion of value V' and let B'"
                     " be the string or binary portion of the value V'.\n"
                     "\n"
                     "     - If O is not equal to O' and O is not equal to"
                     " the rootDSE, continue processing other values of V'.\n"
                     "\n"
                     "     - If B is not equal to the initial bytes of B',"
                     " continue processing other values of V'. Note"
                     " especially that only byte values are used in this"
                     " comparison. No special handling of B as a string is"
                     " performed (for example, no case-insensitivity, locale"
                     " specific comparisons, etc.).\n"
                     "\n"
                     "     - Return true.\n"
                     "\n"
                     "   * If this method does not return true, it returns false.\n",
   .notes         =  NULL,
};



